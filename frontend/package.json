{
  "name": "@tauri-vuetify-starter/frontend",
  "version": "0.0.0",
  "description": "A modern desktop application built with Tauri v2 and Vue 3",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev:tauri": "cd .. && RUST_LOG=info CARGO_INCREMENTAL=1 tauri dev --config backend/tauri.conf.json",
    "clean": "rimraf ./node_modules/.vite",
    "build": "run-p type-check \"build-only {@}\" build:tauri --",
    "build:analyze": "vite build --mode analyze",
    "build:clean": "rimraf dist",
    "build:tauri": "tauri build --config ../backend/tauri.conf.json",
    "build:tauri:mac": "tauri build --config ../backend/tauri.conf.json --target universal-apple-darwin",
    "build:tauri:windows-x64": "tauri build --config ../backend/tauri.conf.json --target x86_64-pc-windows-msvc",
    "build:tauri:windows-arm64": "tauri build --config ../backend/tauri.conf.json --target aarch64-pc-windows-msvc",
    "build:tauri:windows-docker-x64": "cd .. && ./scripts/build-windows-docker.sh x64",
    "build:tauri:windows-docker-arm64": "cd .. && ./scripts/build-windows-docker.sh arm64",
    "build:tauri:linux-x64": "tauri build --config ../backend/tauri.conf.json --target x86_64-unknown-linux-gnu",
    "build:tauri:linux-arm64": "tauri build --config ../backend/tauri.conf.json --target aarch64-unknown-linux-gnu",
    "build:tauri:linux-docker-x64": "node -e \"require('child_process').execSync(process.platform === 'win32' ? 'pwsh ..\\\\scripts\\\\build-linux-docker.ps1 -Target x64' : 'bash ../scripts/build-linux-docker.sh x64', {stdio: 'inherit'})\"",
    "build:tauri:linux-docker-arm64": "node -e \"require('child_process').execSync(process.platform === 'win32' ? 'pwsh ..\\\\scripts\\\\build-linux-docker.ps1 -Target arm64' : 'bash ../scripts/build-linux-docker.sh arm64', {stdio: 'inherit'})\"",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "run-s lint:*",
    "lint:oxlint": "oxlint . --fix",
    "lint:eslint": "eslint . --fix --cache --cache-location ./node_modules/.vite/eslint-cache",
    "lint:prettier": "prettier \"./**/*.{js,ts,json,css,sass,scss,htm,html,vue,md}\" -w -u",
    "lint:style": "stylelint \"./**/*.{css,sass,scss,htm,html,vue}\" --fix --cache-location ./node_modules/.vite/stylelint-cache && prettier \"./**/*.{css,sass,scss,htm,html,vue}\" -w -u",
    "tauri": "tauri"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@tauri-apps/api": "^2.10.1",
    "@tauri-apps/plugin-dialog": "^2.6.0",
    "@tauri-apps/plugin-fs": "^2.4.5",
    "@tauri-apps/plugin-http": "^2.5.7",
    "@tauri-apps/plugin-notification": "^2.3.3",
    "@tauri-apps/plugin-opener": "^2.5.3",
    "@tauri-apps/plugin-os": "^2.3.2",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "three": "^0.183.1",
    "unified-network": "^0.6.4",
    "vue": "^3.5.29",
    "vue-i18n": "^11.2.8",
    "vuetify": "^4.0.0"
  },
  "devDependencies": {
    "@intlify/eslint-plugin-vue-i18n": "^4.2.0",
    "@intlify/unplugin-vue-i18n": "^11.0.7",
    "@modyfi/vite-plugin-yaml": "^1.1.1",
    "@tauri-apps/cli": "^2.10.0",
    "@tsconfig/node-lts": "^24.0.0",
    "@types/node": "^25.3.2",
    "@types/three": "^0.183.1",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.7.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^10.0.2",
    "eslint-import-resolver-custom-alias": "^1.3.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-oxlint": "^1.50.0",
    "eslint-plugin-vue": "^10.8.0",
    "eslint-plugin-vuejs-accessibility": "^2.5.0",
    "eslint-plugin-vuetify": "^2.5.3",
    "eslint-plugin-yml": "^3.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.7",
    "npm-run-all2": "^8.0.4",
    "oxlint": "^1.50.0",
    "postcss-html": "^1.8.1",
    "prettier": "^3.8.1",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^6.1.3",
    "rollup-plugin-visualizer": "^7.0.0",
    "sass-embedded": "^1.97.3",
    "stylelint": "^17.4.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended-scss": "^17.0.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-order": "^7.0.1",
    "stylelint-scss": "^7.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1",
    "vite": "^7.3.1",
    "vite-plugin-checker": "0.12.0",
    "vite-plugin-vue-devtools": "^8.0.6",
    "vite-plugin-vuetify": "^2.1.3",
    "vue-tsc": "^3.2.5",
    "yaml-eslint-parser": "^2.0.0"
  }
}
